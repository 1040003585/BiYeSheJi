import java.io.IOException;
import java.io.InputStream;
import java.net.Socket;

public class HTTPSession implements Runnable {

	static long threadCount = 0;
	private Socket mySocket;
	Thread t = null;

	public HTTPSession(Socket s) {
		// TODO Auto-generated constructor stub
		mySocket = s;
		// 为该次会话建立一个Daemon线程
		t = new Thread(this);
		t.setDaemon(true);
		t.start();
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println("\t00HTTPSession.run()" + t.getId());
		threadCount++;
		try {
			InputStream isInputStream = mySocket.getInputStream();

		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println("\t11HTTPSession.run()" + t.getId());
		System.out.println("\t11ThreadCount:" + --threadCount);
	}

}
